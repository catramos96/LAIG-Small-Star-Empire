	        <controlpoint t="0" tx="0" ty="0" tz="0" rx="0" ry="0" rz="0" sx="1" sy="1" sz="1"/>
	        <controlpoint t="3" tx="1" ty="1" tz="1" rx="0" ry="0" rz="0" sx="1" sy="1" sz="1"/>
	        <controlpoint t="6" tx="0" ty="0" tz="0" rx="90" ry="0" rz="0" sx="1" sy="1" sz="1"/>
	        <controlpoint t="9" tx="0" ty="0" tz="1" rx="0" ry="0" rz="0" sx="0.5" sy="0.5" sz="0.5"/>
	        <controlpoint t="12" tx="0" ty="0" tz="0" rx="0" ry="90" rz="0" sx="2" sy="2" sz="2"/>
	    </animation>
