        <perspective id="player1" near="0.1" far="500" angle="50">
            <from x="4" y="8" z="13" />
            <to x="3" y="0" z="2" />
        </perspective>
        <perspective id="player2" near="0.1" far="500" angle="50">
            <from x="13" y="9" z="-2" />
            <to x="3" y="0" z="2" />
